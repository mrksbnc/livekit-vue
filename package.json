{
  "name": "@mrksbnc/livekit-vue",
  "version": "0.0.5",
  "license": "MIT",
  "author": "Bence Markus",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mrksbnc/livekit-vue.git"
  },
  "exports": {
    ".": {
      "types": "./dist/src/index.d.ts",
      "import": "./dist/livekit-vue.js",
      "require": "./dist/livekit-vue.umd.cjs"
    },
    "./index.css": {
      "import": "./dist/index.css",
      "require": "./dist/index.css"
    },
    "./composables": {
      "import": "./dist/composables.js",
      "require": "./dist/composables.umd.cjs"
    }
  },
  "main": "dist/livekit-vue.umd.cjs",
  "module": "dist/livekit-vue.js",
  "types": "dist/src/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite --config vite.config.local.ts",
    "build": "run-p type-check \"build:clean {@}\" --",
    "build:clean": "vite build",
    "build:prod": "vite optimize --force && vite build --mode production",
    "preview": "vite preview",
    "test:unit": "vitest --coverage",
    "type-check": "vue-tsc --noEmit",
    "lint:oxlint": "oxlint . --fix -D correctness --ignore-path .gitignore",
    "lint:eslint": "eslint . --fix",
    "lint": "run-s lint:*",
    "preinstall": "npx only-allow pnpm",
    "format": "prettier --write src/",
    "format:check": "prettier --check src/"
  },
  "dependencies": {
    "@livekit/components-core": "^0.12.2",
    "@livekit/protocol": "^1.36.1",
    "@vueuse/core": "^13.0.0",
    "livekit-client": "^2.9.9",
    "vue": "^3.5.13"
  },
  "devDependencies": {
    "@changesets/cli": "^2.28.1",
    "@tailwindcss/vite": "^4.0.15",
    "@tsconfig/node22": "^22.0.0",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.14.0",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vitest/coverage-v8": "3.0.9",
    "@vitest/eslint-plugin": "^1.1.36",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.5.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.21.0",
    "eslint-plugin-oxlint": "^0.16.5",
    "eslint-plugin-vue": "~10.0.0",
    "jiti": "^2.4.2",
    "jsdom": "^26.0.0",
    "npm-run-all2": "^7.0.2",
    "oxlint": "^0.16.3",
    "prettier": "3.5.3",
    "prettier-plugin-organize-imports": "^4.1.0",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "tailwindcss": "^4.0.17",
    "typescript": "~5.8.0",
    "vite": "^6.2.5",
    "vite-plugin-dts": "^4.5.3",
    "vite-plugin-vue-devtools": "^7.7.2",
    "vite-svg-loader": "^5.1.0",
    "vitest": "^3.0.8",
    "vue-tsc": "^2.2.8"
  },
  "packageManager": "pnpm@10.6.5",
  "engines": {
    "node": ">=22"
  }
}
